    "dialogueList": [
        {
            "dialogue": {
                "EncounterObjectGuid": "73df8d9c-a274-48fd-98c9-2bd0d7860e83"
            },
            "dialogueContent": [
                {
                    "audioName": "NONE",
                    "cameraDistance": "Far",
                    "cameraFocusGuid": "",
                    "cameraHeight": "Default",
                    "emote": "Default",
                    "revealRadius": -1,
                    "selectedCastDefId": "castDef_FP_AgentX",
                    "words": "Max has always been reliable source. Presently, he has an archive of the targets these pirates have been hitting and their future goals.",
                    "wordsColor": {
                        "a": 1,
                        "b": 1,
                        "g": 1,
                        "r": 1
                    }
                },
                {
                    "audioName": "NONE",
                    "cameraDistance": "Far",
                    "cameraFocusGuid": "",
                    "cameraHeight": "Default",
                    "emote": "Default",
                    "revealRadius": -1,
                    "selectedCastDefId": "castDef_DariusDefault",
                    "words": "It's about time we find out what 'the Queen' is after.",
                    "wordsColor": {
                        "a": 1,
                        "b": 1,
                        "g": 1,
                        "r": 1
                    }
                },
                {
                    "audioName": "NONE",
                    "cameraDistance": "Far",
                    "cameraFocusGuid": "a85da736-7a2e-4cc6-aff3-feb9d73fc7e1",
                    "cameraHeight": "Default",
                    "emote": "Default",
                    "revealRadius": -1,
                    "selectedCastDefId": "castDef_FP_AgentX",
                    "words": "Whether or not this leads to any conclusive evidence is beyond me. However, it would be a great boon to have a point of interest.",
                    "wordsColor": {
                        "a": 1,
                        "b": 1,
                        "g": 1,
                        "r": 1
                    }
                },
                {
                    "audioName": "NONE",
                    "cameraDistance": "Far",
                    "cameraFocusGuid": "a85da736-7a2e-4cc6-aff3-feb9d73fc7e1",
                    "cameraHeight": "Default",
                    "emote": "Default",
                    "revealRadius": -1,
                    "selectedCastDefId": "castDef_SE01_CommVoice01",
                    "words": "Waiting on pickup, sir. All important intel memorised.",
                    "wordsColor": {
                        "a": 1,
                        "b": 1,
                        "g": 1,
                        "r": 1
                    }
                },
                {
                    "audioName": "NONE",
                    "cameraDistance": "Far",
                    "cameraFocusGuid": "a85da736-7a2e-4cc6-aff3-feb9d73fc7e1",
                    "cameraHeight": "Default",
                    "emote": "Default",
                    "revealRadius": -1,
                    "selectedCastDefId": "castDef_SE01_CommVoice01",
                    "words": "If you can get my datapad though, we will even have more information to play with. Always the possibility I missed something.",
                    "wordsColor": {
                        "a": 1,
                        "b": 1,
                        "g": 1,
                        "r": 1
                    }
                }
            ],
            "name": "Dialogue_MissionStart",
            "overrideDialogueBucketId": ""
        },
        {
            "dialogue": {
                "EncounterObjectGuid": "4011a4c3-cba2-4d22-b2b3-3b19a3297ab9"
            },
            "dialogueContent": [
                {
                    "audioName": "NONE",
                    "cameraDistance": "Far",
                    "cameraFocusGuid": "",
                    "cameraHeight": "Default",
                    "emote": "Default",
                    "revealRadius": -1,
                    "selectedCastDefId": "castDef_DariusDefault",
                    "words": "Welcome aboard Max, you look like you've seen better days.",
                    "wordsColor": {
                        "a": 1,
                        "b": 1,
                        "g": 1,
                        "r": 1
                    }
                },
                {
                    "audioName": "NONE",
                    "cameraDistance": "Far",
                    "cameraFocusGuid": "",
                    "cameraHeight": "Default",
                    "emote": "Default",
                    "revealRadius": -1,
                    "selectedCastDefId": "castDef_SE01_CommVoice01",
                    "words": "Today may be a much better day than tomorrow, if what I found out is true.",
                    "wordsColor": {
                        "a": 1,
                        "b": 1,
                        "g": 1,
                        "r": 1
                    }
                },
                {
                    "audioName": "NONE",
                    "cameraDistance": "Far",
                    "cameraFocusGuid": "",
                    "cameraHeight": "Default",
                    "emote": "Default",
                    "revealRadius": -1,
                    "selectedCastDefId": "castDef_FarahDefault",
                    "words": "You poor thing, Lets get you to medical.",
                    "wordsColor": {
                        "a": 1,
                        "b": 1,
                        "g": 1,
                        "r": 1
                    }
                },
                {
                    "audioName": "NONE",
                    "cameraDistance": "Far",
                    "cameraFocusGuid": "",
                    "cameraHeight": "Default",
                    "emote": "Default",
                    "revealRadius": -1,
                    "selectedCastDefId": "castDef_SE01_CommVoice01",
                    "words": "Soon I have to talk to Jac- Agent X first, *Coughs, Wheezing*.",
                    "wordsColor": {
                        "a": 1,
                        "b": 1,
                        "g": 1,
                        "r": 1
                    }
                }
            ],
            "name": "Dialogue_MissionSuccess",
            "overrideDialogueBucketId": ""
        },
        {
            "dialogue": {
